<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ISP.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ISP"
    Title="ISP">

    <FlyoutItem Title="1st lab">

        <ShellContent
                ContentTemplate="{DataTemplate local:CalculatorPage}"
                Route="CalculatorPage" />

    </FlyoutItem>
    
    
    <FlyoutItem Title="2nd lab">

        <ShellContent
                Title="ProgressDemo"
                ContentTemplate="{DataTemplate local:IntegralPage}"
                Route="IntegralPage" />

    </FlyoutItem>

    <FlyoutItem Title="3rd lab">

        <ShellContent
                Title="SpaProcedures"
                ContentTemplate="{DataTemplate local:SpaPage}"
                Route="SpaPage" />
    </FlyoutItem>

    <FlyoutItem Title="4th lab">

        <ShellContent
                Title="CurrencyConverter"
                ContentTemplate="{DataTemplate local:CurrencyConverterPage}"
                Route="CurrencyConverterPage"
            />

    </FlyoutItem>

</Shell>
